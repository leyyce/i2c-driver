/dts-v1/;
/plugin/;

/ {
    fragment@0 {
        target = <&i2c1>;
        __overlay__ {
            #address-cells = <1>;
            #size-cells = <0>;
            
            monitoring_system: monitoring_system@10 {
                compatible = "embedded_linux,monitoring_system";
                reg = <0x10>;
                status = "okay";
            };
        };
    };
};